{
  "name": "spend-snap",
  "type": "module",
  "scripts": {
    "//dev": "concurrently --names 'hono,vike' 'bun run --watch src/server/index.ts' 'bunx --bun vite'",
    "dev": "bunx --bun vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dz:seed": "bun run scripts/seed.ts",
    "dz:up": "bun run scripts/migrate.ts",
    "dz:generate": "bun drizzle-kit generate:sqlite",
    "dz:create": "bun drizzle-kit generate:sqlite --custom",
    "dz:studio": "bun drizzle-kit studio",
    "dz:check": "bun drizzle-kit check:sqlite",
    "dz:drop": "bun drizzle-kit drop",
    "dz:push": "drizzle-kit push:sqlite"
  },
  "dependencies": {
    "@felte/reporter-solid": "^1.2.10",
    "@felte/solid": "^1.2.13",
    "@felte/validator-superstruct": "^1.0.15",
    "@hono/typebox-validator": "^0.2.4",
    "@libsql/client": "^0.6.0",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@nanostores/solid": "^0.4.2",
    "@node-rs/argon2": "^1.8.3",
    "@sinclair/typebox": "^0.32.27",
    "autoprefixer": "^10.4.19",
    "drizzle-orm": "^0.30.9",
    "hono": "^4.2.9",
    "lucia": "^3.2.0",
    "nanostores": "^0.10.3",
    "nprogress": "^0.2.0",
    "postcss": "^8.4.38",
    "solid-js": "^1.8.16",
    "superstruct": "^1.0.4"
  },
  "devDependencies": {
    "@hono/vite-dev-server": "^0.12.0",
    "@types/bun": "^1.1.1",
    "@types/nprogress": "^0.2.3",
    "daisyui": "^4.10.2",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.17",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.5",
    "unplugin-icons": "^0.18.5",
    "vike": "^0.4.171",
    "vike-solid": "^0.4.5",
    "vite": "^5.2.10",
    "tailwindcss": "^3.4.3"
  }
}
