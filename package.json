{
  "name": "spend-snap",
  "type": "module",
  "scripts": {
    "//dev": "concurrently --names 'hono,vike' 'bun run --watch src/server/index.ts' 'bunx --bun vite'",
    "dev": "bunx --bun vite --host",
    "build": "tsc && vite build",
    "preview": "NODE_ENV=production bun run src/server/index.ts",
    "s3:create": "docker-compose up -d",
    "dz:seed": "bun run scripts/seed.ts",
    "dz:migrate": "bun run scripts/migrate.ts",
    "dz:generate": "bun drizzle-kit generate",
    "dz:create": "bun drizzle-kit generate --custom",
    "dz:studio": "bun drizzle-kit studio",
    "dz:check": "bun drizzle-kit check",
    "dz:drop": "bun drizzle-kit drop",
    "dz:push": "drizzle-kit push",
    "dz:backup-local": "cp local.db local-backup.db",
    "dz:use-local-backup": "cp local-backup.db local.db",
    "pm2": "pm2 start bun --name 'spend-snap' -- run preview",
    "pm2-stop": "pm2 stop spend-snap && pm2 delete spend-snap",
    "redeploy": "bun pm2-stop && bun run build && bun run pm2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.577.0",
    "@aws-sdk/s3-request-presigner": "^3.577.0",
    "@felte/reporter-solid": "^1.2.10",
    "@felte/solid": "^1.2.13",
    "@felte/validator-superstruct": "^1.0.15",
    "@hono/typebox-validator": "^0.2.4",
    "@iconify-json/lets-icons": "^1.1.2",
    "@libsql/client": "^0.6.0",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@nanostores/solid": "^0.4.2",
    "@node-rs/argon2": "^1.8.3",
    "@paralleldrive/cuid2": "^2.2.2",
    "@scalar/hono-api-reference": "^0.5.42",
    "@sinclair/typebox": "^0.32.30",
    "@solid-primitives/presence": "^0.0.6",
    "@solid-primitives/tween": "^1.3.0",
    "@tanstack/solid-query": "^5.37.1",
    "@tanstack/solid-query-devtools": "^5.37.1",
    "@tanstack/solid-table": "^8.17.3",
    "autoprefixer": "^10.4.19",
    "chart.js": "^4.4.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.11",
    "drizzle-orm": "^0.30.10",
    "hono": "^4.3.7",
    "lucia": "^3.2.0",
    "nanostores": "^0.10.3",
    "nprogress": "^0.2.0",
    "numerable": "^0.3.15",
    "postcss": "^8.4.38",
    "rand-seed": "^1.0.2",
    "solid-js": "^1.8.17",
    "solid-sonner": "npm:@blankeos/solid-sonner",
    "superstruct": "^1.0.4",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@hono/vite-dev-server": "^0.12.1",
    "@iconify-json/fluent-emoji": "^1.1.18",
    "@iconify-json/line-md": "^1.1.37",
    "@iconify-json/material-symbols": "^1.1.80",
    "@iconify-json/mdi": "^1.1.66",
    "@iconify-json/mingcute": "^1.1.17",
    "@iconify-json/solar": "^1.1.9",
    "@types/bun": "^1.1.2",
    "@types/nprogress": "^0.2.3",
    "daisyui": "^4.11.1",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.21.1",
    "nodemon": "^3.1.0",
    "s3rver": "^3.7.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "unplugin-icons": "^0.19.0",
    "vike": "^0.4.171",
    "vike-solid": "^0.4.5",
    "vite": "^5.2.11"
  }
}
